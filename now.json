{
  "version": 2,
  "builds": [
    {
      "src": "backend/index.js",
      "use": "@now/node-server"
    },
    {
      "src": "frontend/next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
